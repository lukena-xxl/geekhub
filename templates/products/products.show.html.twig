<table cellpadding="10">
    <tr>
        <th>ID</th>
        <th>Product Title</th>
        <th>Category Title (ID)</th>
        <th>Price</th>
        <th>Stock</th>
    </tr>
    {% for product in products %}
        <tr>
            <td>{{ product.id }}</td>
            <td>{{ product.title }}</td>
            <td>{{ categories[product.id_category] }}</td>
            <td>{{ product.price }}</td>
            <td>{{ product.stock }}</td>
        </tr>
    {% endfor %}
</table>